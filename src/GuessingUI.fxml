<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="170.0" prefWidth="662.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GuessingController">
   <children>
      <TextField fx:id="textfield" alignment="CENTER" layoutX="24.0" layoutY="69.0" onAction="#handleGuess" prefHeight="43.0" prefWidth="243.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="15.0" layoutY="14.0" text="Guessing Game">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button layoutX="24.0" layoutY="124.0" mnemonicParsing="false" onAction="#handleGuess" text="Guess" />
      <Button layoutX="104.0" layoutY="124.0" mnemonicParsing="false" onAction="#handleClear" text="Clear" />
      <Button layoutX="175.0" layoutY="124.0" mnemonicParsing="false" onAction="#newGame" text="New game" />
      <Label fx:id="label1" layoutX="293.0" layoutY="69.0" prefHeight="43.0" prefWidth="318.0">
         <font>
            <Font size="24.0" />
         </font></Label>
      <Label fx:id="label2" layoutX="298.0" layoutY="122.0" prefHeight="31.0" prefWidth="219.0" text="Total guess: ">
         <font>
            <Font size="24.0" />
         </font></Label>
   </children>
</AnchorPane>
